{
  "chmod": "0755",
  "directories": [
    "bin",
    "config",
    "resources",
    "src"
  ],
  "files": [
    "constants.php",
    "vendor/autoload.php",
    "vendor/symfony/console/Resources/bin/hiddeninput.exe"
  ],
  "finder": [
    {
      "in": "vendor",
      "name": "*.php",
      "exclude": [
        "phpunit",
        "Tests",
        "tests"
      ]
    }
  ],
  "main": "bin/platform",
  "output": "platform.phar",
  "stub": "resources/phar-stub.php",
  "compactors": [
      "KevinGH\\Box\\Compactor\\Json",
      "KevinGH\\Box\\Compactor\\Php"
  ]
}
