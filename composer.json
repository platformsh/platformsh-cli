{
    "name": "platformsh/cli",
    "description": "Platform.sh CLI",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "doctrine/cache": "^1.7",
        "platformsh/console-form": ">=0.0.22 <2.0",
        "platformsh/client": "^2.1",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/process": "^4.0",
        "stecman/symfony-console-completion": "~0.10",
        "symfony/event-dispatcher": "^4.0",
        "padraic/phar-updater": "^1.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0",
        "ext-json": "*"
    },
    "suggest": {
        "drush/drush": "For Drupal projects"
    },
    "autoload": {
        "files": ["constants.php"],
        "psr-4": {
            "Platformsh\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Platformsh\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "bamarni/composer-bin-plugin": "^1.2"
    },
    "authors": [
        {
            "name": "Platform.sh"
        },
        {
            "name": "Patrick Dawkins"
        },
        {
            "name": "Bojan Zivanovic"
        }
    ],
    "bin": [
        "bin/platform"
    ],
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    }
}
